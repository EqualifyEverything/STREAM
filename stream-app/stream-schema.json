{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "STREAM Schema",
  "type": "object",
  "properties": {
    "messages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "message": { "type": "string" },
          "relatedTagIds": {
            "type": "array",
            "items": { "type": "integer" }
          },
          "relatedcodeIds": {
            "type": "array",
            "items": { "type": "integer" }
          },
          "relatedPageIds": {
            "type": "array",
            "items": { "type": "integer" }
          },
          "relatedStatusId": { "type": "integer" }
        },
        "required": ["message", "relatedTagIds", "relatedcodeIds", "relatedPageIds"]
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tagId": { "type": "integer" },
          "tag": { "type": "string" }
        },
        "required": ["tagId", "tag"]
      }
    },
    "code": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "codeId": { "type": "integer" },
          "code": { "type": "string" }
        },
        "required": ["codeId", "code"]
      }
    },
    "pages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "pageId": { "type": "integer" },
          "pageUrl": { "type": "string" }
        },
        "required": ["pageId", "pageUrl"]
      }
    },
    "statuses": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "statusId": { "type": "integer" },
          "status": { "type": "string" }
        },
        "required": ["statusId", "status"]
      }
    }
  },
  "required": ["messages", "tags", "code", "pages", "statuses"]
}
